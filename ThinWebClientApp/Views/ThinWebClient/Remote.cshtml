@model ThinWebClientModelRemote
@{
    Page.Title = $"\"{Model.ConnectOptions!.Pcid}\" のリモートデスクトップ";
}
<h2 class="title is-4">@Page.Title</h2>

<!-- Display -->
<div id="display"></div>

@section Scripts{

    <!-- Init -->
    <script>
        Main.ThinWebClient_Remote_PageLoad(window, document, "@Model.WebSocketUrl", "@Model.SessionId", "@Model.ConnectOptions.Pcid._JavaScriptSafeStrEncode()", "@Model.SvcType.ToString()", "@Model.Profile._ObjectToJson()._JavaScriptEasyStrEncrypt("easyJsonEncode")");
    </script>

}

